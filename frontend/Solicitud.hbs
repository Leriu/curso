<link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="/javascripts/solicitud/solicitud.js" type="text/javascript"></script>
<!------ Include the above in your HEAD tag ---------->

<div class="container">
	<div class="row">
		<div class="span12">
    	   <table class="table-condensed table-bordered table-striped">
                <thead>
                    <tr>
                    <th colspan="7" >
                        <div class="btn-group-vertical" id="atraadelmes" >
                                <a class="btn" id="btnatrasm" onclick="atrasmes()" ><i class="icon-chevron-left" ></i></a>
                                <a class="btn" id="btnadelantem" onclick="adelmes()" ><i class="icon-chevron-right"></i></a>
                        </div>
                        <a class="btn" id="btnmes" onclick="meses()"style="border-bottom-left-radius:4px; border-bottom-right-radius:4px; border-bottom-style:solid; border-bottom-width:1px; border-collapse:separate; border-image-outset:0; border-image-repeat:stretch;border-image-slice:100%; border-image-source:none; border-image-width:1;padding-bottom:4px; padding-left:6px; padding-right:6px; padding-top:4px;width: 206px; margin-bottom:10px;">Mes</a>
                            <select class="selectpicker show-tick" style="display: none;" id="btnGroupDropmes" >
                                <optgroup>
                                    <option value="">Seleccione mes</option>
                                    <option value="Enero">Enero</option>
                                    <option value="Febrero">Febrero</option>
                                    <option value="Marzo">Marzo</option>
                                    <option value="Abril">Abril</option>
                                    <option value="Mayo">Mayo</option>
                                    <option value="Junio">Junio</option>
                                    <option value="Julio">Julio</option>
                                    <option value="Agosto">Agosto</option>
                                    <option value="Septiembre">Septiembre</option>
                                    <option value="Octubre">Octubre</option>
                                    <option value="Noviembre">Noviembre</option>
                                    <option value="Diciembre">Diciembre</option>
                                </optgroup>
                            </select>
                        <a class="btn" id="btnaño" onclick="años()" style="border-bottom-left-radius:4px; border-bottom-right-radius:4px; border-bottom-style:solid; border-bottom-width:1px; border-collapse:separate; border-image-outset:0; border-image-repeat:stretch;border-image-slice:100%; border-image-source:none; border-image-width:1;padding-bottom:4px; padding-left:6px; padding-right:6px; padding-top:4px;width: 206px; margin-bottom:10px;">Año</a>
                            <select class="selectpicker show-tick" style="display: none;" id="btnGroupDroAño" >
                                <optgroup>
                                    <option value="">Seleccione año</option>
                                    <option value="2020">2020</option>
                                    <option value="2021">2021</option>
                                    <option value="2022">2022</option>
                                    <option value="2023">2023</option>
                                    <option value="2024">2024</option>
                                    <option value="2025">2025</option>
                                    <option value="2026">2026</option>
                                    <option value="2027">2027</option>
                                    <option value="2028">2028</option>
                                    <option value="2029">2029</option>
                                    <option value="2030">2030</option>
                                    <option value="2031">2031</option>
                                </optgroup>
                            </select>
                            <div class="btn-group-vertical" id="atraadelaño"> 
                                <a class="btn" id="btnatrasa" onclick="atrasaño()" ><i class="icon-chevron-left"></i></a>
                                <a class="btn" id="btnadelantea" onclick="adelaño()" ><i class="icon-chevron-right"></i></a>
                            </div>
                        <a class="btn" id="fechamesaño" onclick="damesem()" style="border-bottom-left-radius:4px; border-bottom-right-radius:4px; border-bottom-style:solid; border-bottom-width:1px; border-collapse:separate; border-image-outset:0; border-image-repeat:stretch;border-image-slice:100%; border-image-source:none; border-image-width:1;padding-bottom:4px; padding-left:6px; padding-right:6px; padding-top:4px;width: 206px; margin-bottom:10px;">Muestra fechas</a>
                    </th>
                    </tr>
                    <tr>
                        <th>Domingo</th>
                        <th>Lunes</th>
                        <th>Martes</th>
                        <th>Miércoles</th>
                        <th>Jueves</th>
                        <th>Viernes</th>
                        <th>Sábado</th>
                    </tr>
                </thead>
                <tbody >
                        <tr>
                            {{#each dias}}
                                {{#inicioMes inMes }}
                                    <td> {{this.dia}}</td>
                                {{/inicioMes}}
                            {{/each}}
                        </tr>
                </tbody>
            </table>   
        </div>  
    </div>
</div>
		



</div>
